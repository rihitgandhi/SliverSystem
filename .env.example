## Copy this file to `.env` and fill in the values. DO NOT commit the real `.env` to git.
# Example .env for local development

# Application environment: development or production
APP_ENV=development

# Google Gemini API key (required in production)
GEMINI_API_KEY=your-google-gemini-api-key-here

# Flask session secret (use a strong random value in production)
FLASK_SECRET_KEY=change-me-to-a-long-random-string

# Optional: run server host and port
HOST=0.0.0.0
PORT=5000

# Flask debug mode (set to False in production)
FLASK_DEBUG=True
